{
  "name": "sonarqube-js-example",
  "version": "1.0.0",
  "description": "Exemplo de projeto JS para testar a conex√£o com SonarQube.",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --ci --json --outputFile=./test-results.json --testLocationInResults --reporters=default --reporters=jest-junit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": ["lcov", "text"],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**"
    ],
    "reporters": [
      "default",
      ["jest-junit", { "outputDirectory": "./", "outputName": "junit.xml" }]
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ]
  }
}
